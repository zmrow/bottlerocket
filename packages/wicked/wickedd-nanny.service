[Unit]
Description=wicked network nanny service
Requisite=dbus.service
After=local-fs.target dbus.service network-pre.target
Before=wicked.service network.target
PartOf=wickedd.service

[Service]
Type=notify
LimitCORE=infinity
ExecStart=/usr/sbin/wickedd-nanny --systemd --foreground --log-level debug --debug most
StandardError=null
Restart=on-abort
